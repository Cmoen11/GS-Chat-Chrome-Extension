{
  "manifest_version": 2,

  "name": "GS Chat",
  "description": "En applikasjon som gjør det enklere å rolle på galtvortskolen.net",
  "version": "1.4",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://galtvortskolen.net/"
  ]
}